{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
      },
    "extends": [
        "airbnb",
        "airbnb/hooks",
        "eslint-config-prettier",
        "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "parser": "babel-eslint",
    "rules": {
        "no-plusplus": "off",
        "no-console": "off",
        "react/prop-types": "off",
        "camelcase": "off",
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
        "react/jsx-filename-extension": "off",
        "react-hooks/exhaustive-deps": "off",
        "react/jsx-props-no-spreading": ["off"],
        "class-methods-use-this": "off",
        "react/self-closing-comp": "off",
        "react/jsx-boolean-value": "off",
        "import/order": "off",
        "no-unused-vars": "off"
      },
    "settings": {
        "react": {
          "pragma": "React",
          "version": "detect"
        }
      }
}
